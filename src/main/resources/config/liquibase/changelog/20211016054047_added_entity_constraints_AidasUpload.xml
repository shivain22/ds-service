        <addForeignKeyConstraint baseColumnNames="status_modified_by_id"
                                 baseTableName="aidas_upload"
                                 constraintName="fk_aidas_upload__status_modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aidas_user"/>
    </changeSet>
</databaseChangeLog>
